<ul>
  {% for item in items %}
    <li class="{{ item.resourceKey }}__menu-item">
      {% if item.url %}
        <a href="{{ item.url }}" title="{{ item.title }}">{{ item.title }}</a>
      {% else %}
        {{ item.title }}
      {% endif %}
      {% if item.hasChildren %}
        {% include '@SuluMegamenu/section.html.twig' with {'items': item.children } %}
      {% endif %}
    </li>
  {% endfor %}
</ul>
